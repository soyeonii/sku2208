package day02;

public class User {
	public static void run(Car c) {
		c.run();
	}
	
	public static void main(String[] args) {
		// 부모는 자식을 가리킬 수 있다.
		Car c1 = new Bus();
		Car c2 = new SportsCar();

		// 오류
		// Bus b1 = new Car();

		c2.run();
		((SportsCar) c2).open(); // 형 변환
		
		run(c1);
		run(c2);
	}
}
